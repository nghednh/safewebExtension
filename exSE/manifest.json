{
  "manifest_version": 3,
  "name": "Safe Web",
  "version": "1.0",
  "description": "Block unwanted content and browsers.",
  "permissions": ["webRequest", "webRequestBlocking", "declarativeNetRequest"],
  "host_permissions": ["*://*.tiktok.com/*", "*://*.facebook.com/*"],
  "background": {
    "service_worker": "background.js"
  }
}
